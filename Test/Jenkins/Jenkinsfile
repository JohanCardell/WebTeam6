pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/ahmadgbg/WebTeam6.git'
            }
        }
        stage ('Build') {
			bat 'nuget restore WebTeam6.sln'
			bat "\"${tool 'MSBuild'}\" WebTeam6.sln /p:Configuration=Release /p:Platform=\"Any CPU\" /p:ProductVersion=1.0.0.${env.BUILD_NUMBER}"
		}
	}
	
    post {
        always {
                
        }
    }
}